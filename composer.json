{
    "name": "sitewards/magento-prometheus",
    "description": "Expose some service level Magento metrics on Prometheus",
    "version": "1.0.0",
    "type": "magento-module",
    "homepage": "https://github.com/sitewards/magento-prometheus/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Andrew Howden",
            "email": "hello@andrewhowden.com",
            "homepage": "https://www.andrewhowden.com/",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "hello@andrewhowden.com",
        "issues": "https://github.com/sitewards/magento-prometheus/issues",
        "wiki": "https://github.com/sitewards/magento-prometheus/wiki",
        "source": "https://github.com/sitewards/magento-prometheus"
    }, 
    "require": {
        "jimdo/prometheus_client_php": "v0.9.0"
    },
    "require-dev": {
    },
    "suggest": {
        "fontis/composer-autoloader": "Allows the use of the third party prometheus library with Magento"
    },
    "autoload": {
        "psr-4": {
            "sitewards\\": "src/"
        }
    },
    "archive": {
        "exclude": [
            "test",
            "hack",
            "hooks",
            ".arclint"
        ]
    }
}
